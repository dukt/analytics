{
  "name": "analytics",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "webpack --node-env=production",
    "serve": "webpack-dev-server --node-env=development",
    "lint": "eslint src/ --ext .js,.vue",
    "test": "npx playwright test",
    "test:watch": "chokidar 'playwright/tests/*.spec.js' -c 'yarn test:playwright -- --retries=0 {path}'"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.16.5",
    "@playwright/test": "^1.20.1",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@vue/cli-plugin-babel": "^5.0.3",
    "autoprefixer": "^10.4.4",
    "axios": "^0.26.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "chokidar-cli": "^3.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "clone-deep": "^4.0.1",
    "core-js": "^3.6.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-plugin-vue": "^8.5.0",
    "eslint-webpack-plugin": "^3.1.1",
    "lodash.debounce": "^4.0.8",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.12",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "tailwindcss": "^3.0.23",
    "vue": "^2.6.14",
    "vue-loader": "^15.9.8",
    "vue-select": "^3.18.3",
    "vue-template-compiler": "^2.6.14",
    "vuex": "^3.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "ignorePatterns": [
      "./node_modules/**",
      "./dist/*.js"
    ],
    "overrides": [
      {
        "files": [
          "*.vue"
        ],
        "rules": {
          "vue/multi-word-component-names": "off"
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
