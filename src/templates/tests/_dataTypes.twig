{% extends "analytics/_layouts/tests" %}
{% import "_includes/forms" as forms %}

{% set selectedTab = 'dataTypes' %}

{% set content %}

    <h2>{{ "Data Types"|t('analytics') }}</h2>

    <ul>
        {% for dataType in dataTypes %}
            <li>{{ dataType }}</li>
        {% endfor %}
    </ul>

    <h2>{{ "Google Analytics Data Types"|t('analytics') }}</h2>

    <ul>
        {% for dataType in googleAnalyticsDataTypes %}
            <li>{{ dataType }}</li>
        {% endfor %}
    </ul>

{% endset %}