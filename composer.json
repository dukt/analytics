{
    "name": "dukt/analytics",
    "description": "Customizable statistics widgets and entry tracking for Google Analytics.",
    "version": "5.0.0",
    "type": "craft-plugin",
    "license": "proprietary",
    "keywords": [
        "google",
        "analytics",
        "google analytics",
        "craftcms"
    ],
    "authors": [
        {
            "name": "Dukt",
            "homepage": "https://dukt.net/"
        }
    ],
    "support": {
        "email": "support@dukt.net",
        "issues": "https://github.com/dukt/analytics/issues?state=open",
        "source": "https://github.com/dukt/analytics",
        "docs": "https://github.com/dukt/analytics",
        "rss": "https://github.com/dukt/analytics/commits/v2.atom"
    },
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0.0",
        "league/oauth2-client": "^2.5.0",
        "league/oauth2-google": "^4.0.0",
        "google/apiclient": "^v2.6.0",
        "nystudio107/craft-plugin-vite": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "dukt\\analytics\\": "src/"
        }
    },
    "extra": {
        "name": "Analytics",
        "handle": "analytics",
        "documentationUrl": "https://dukt.net/docs/analytics",
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "require-dev": {
        "craftcms/phpstan": "dev-main"
    },
    "scripts": {
        "phpstan": "phpstan --memory-limit=1G"
    }
}
